					<h2>Hello! My name is</h2>
				</div>
			
				<h1 class="landing-name notActive"><a href="/about"><span>JARED SUASIN</span>.</a></h1>

				<div class="landing-description">
					<p>Iâ€™m a designer and front-end developer who loves creating insightful product design stories and implementing their solutions. Previously at <a target="_blank" href='https://www.dexcom.com/'>Dexcom</a> and <a target="_blank" href='https://www.zillowgroup.com/'>Zillow Group</a>.</p>
				</div>

				<div class="link-container">
					<a id="work"><span>Take a look at what I've been working on</a> <span style="font-size: 1.2rem"> &#8628;</span>
				</div>
			</div><!-- landing-container -->

			<!-- Header -->
			<div id="navbar" class="hidden navbar-container">

				<div id="full-name" class="name-container">
					<a class="notActive" href="/"><h1><span>JARED SUASIN</span></h1></a>
				</div>

				<div id="initials" class="name-container">
					<a class="notActive" href="/"><h1><span>JS</span></h1></a>
				</div>

				<ul class="nav-container">
					<li id="resume" class="nav-item"><a class="notActive" target="_blank" href="/resume"><span>Resume</span></a></li>
					<li id="about" class="nav-item"><a class="notActive" href="/about"><span>About</span></a></li>
					<li id="work" class="nav-item"><a class="active"><span>Work</span></a></li>
				</ul>
			</div>
		</div>

		<div class="page-container">
			<div class="project-block-container">
				{{#each projects}}
					<div class="project-row-container">

						<div class="project-row-info-container-desktop">
							<div class="project-row-info">
								<h2><a href="/{{url}}">{{title}}</a></h2>
								<h6 class="project-type">{{type}}</h6>
								<h6>{{{tags}}}</h6>
							</div>
						</div>
						<div class="project-row-image-container">
							<a href="/{{url}}">
								<div class="project-row-image" style="background-image:url({{src-cover}});background-position-y:{{position-y-home}};background-position-x:{{position-x-home}};"></div>
							</a>
						</div>
						<div class="project-row-info-container-mobile">
							<div class="project-row-info">
								<h2><a href="/{{url}}">{{title}}</a></h2>
								<h6 class="project-type">{{type}}</h6>
								<h6>{{{tags}}}</h6>
							</div>
						</div>

					</div>
				{{/each}}
			</div>


		
